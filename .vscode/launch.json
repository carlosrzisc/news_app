{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "[DEVDebug]",
      "program": "${workspaceFolder}/lib/main_development.dart",
      "request": "launch",
      "type": "dart",
      "args": [
        "--flavor",
        "development",
        "--debug",
        "--dart-define-from-file=config.json"
      ]
    },
    {
      "name": "[DEVRelease]",
      "program": "${workspaceFolder}/lib/main_development.dart",
      "request": "launch",
      "type": "dart",
      "args": [
        "--flavor",
        "development",
        "--release",
        "--dart-define=apiLoggerEnabled=false",
        "--dart-define-from-file=config.json"
      ]
    },
    {
      "name": "[STGDebug]",
      "program": "${workspaceFolder}/lib/main_staging.dart",
      "request": "launch",
      "type": "dart",
      "args": [
        "--flavor",
        "staging",
        "--debug",
        "--dart-define-from-file=config.json"
      ]
    },
    {
      "name": "[STGRelease]",
      "program": "${workspaceFolder}/lib/main_staging.dart",
      "request": "launch",
      "type": "dart",
      "args": [
        "--flavor",
        "staging",
        "--release",
        "--dart-define-from-file=config.json"
      ]
    },
    {
      "name": "[PRDDebug]",
      "program": "${workspaceFolder}/lib/main.dart",
      "request": "launch",
      "type": "dart",
      "args": [
        "--flavor",
        "production",
        "--debug",
        "--dart-define-from-file=config.json"
      ]
    },
    {
      "name": "[PRDRelease]",
      "program": "${workspaceFolder}/lib/main.dart",
      "request": "launch",
      "type": "dart",
      "args": [
        "--flavor",
        "production",
        "--release",
        "--dart-define-from-file=config.json"
      ]
    },
  ]
}